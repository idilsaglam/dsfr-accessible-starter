{
  "ci": {
    "collect": { "staticDistDir": "./dist", "url": ["/index.html"] },
    "assert": {
      "assertions": {
        "categories:accessibility": ["error", { "minScore": 0.98 }],
        "color-contrast": "error",
        "duplicate-id": "error"
      }
    },
    "upload": { "target": "temporary-public-storage" }
  }
}
